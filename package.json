{
  "name": "youtube-downloader",
  "version": "1.0.0",
  "private": true,
  "description": "YouTube video downloader built with Next.js and TypeScript",
  "keywords": ["youtube", "downloader", "nextjs", "typescript"],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "prebuild": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.12",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "fluent-ffmpeg": "^2.1.3",
    "youtube-dl-exec": "^3.0.25",
    "@types/fluent-ffmpeg": "^2.1.27"
  }
}